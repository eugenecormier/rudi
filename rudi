  g.write('\\transpose c c' + octave + '{ \clef ' + identclef + ' \\notimesig \override Score.SpacingSpanner #\'spacing-increment = 0 \override Score.SpacingSpanner #\'shortest-duration-space = 0 ')
  temp = list(range(len(WriteAboveWtNotes)))
